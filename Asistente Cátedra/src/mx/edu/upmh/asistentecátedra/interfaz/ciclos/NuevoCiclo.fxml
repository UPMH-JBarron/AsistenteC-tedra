<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111">
   <children>
      <Label fx:id="lblTítulo" text="Nuevo Ciclo" textAlignment="CENTER" />
      <Spinner fx:id="spinAño" editable="true" />
      <Label fx:id="lblMensaje" text="Mensaje" textAlignment="CENTER" />
      <HBox alignment="CENTER">
         <children>
            <Button fx:id="btnCrear" defaultButton="true" mnemonicParsing="false" text="Crear" />
            <Button fx:id="btnCancelar" cancelButton="true" mnemonicParsing="false" text="Cancelar" />
         </children>
      </HBox>
   </children>
</VBox>
